{
    "version": 2,
    "builds": [
        { "src": "app.py", "use": "@vercel/python" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "app.py" }
    ],
    "functions": {
        "app.py": {
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "ignoreFiles": [
        "venv/",
        "__pycache__/",
        "*.pyc",
        "large_files/",
        "node_modules/",
        ".git/"
    ]
}

  